    <style name="lw_AppProgressDialog" parent="Theme.AppCompat.Dialog.Alert">
        
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">@color/lw_txt_color_3</item>
        
        <item name="android:topDark">@android:color/white</item>
        
        <item name="android:bottomDark">@android:color/white</item>
        
        <item name="android:centerDark">@android:color/white</item>
        
        <item name="android:windowIsTranslucent">true</item>

        <item name="android:indeterminateTintMode" ns1:ignore="NewApi">src_atop</item>
        <item name="android:windowMinWidthMinor">80%</item>
        <item name="windowFixedWidthMinor">80%</item>
    </style>
    <style name="lw_EdittextAlertDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/black</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
